  eq ss(fkCfinish2a(S,A,B,CI),A2,B2,I) = ss(S,A2,B2,I) .
  ceq fkCfinish2a(S,A,B,CI) = S 
    if not c-fkCfinish2a(S,A,B,CI) .

-- fake server finish2 - case a
  op c-fkSfinish2a : Sys Prin Prin Cipher -> Bool
  eq c-fkSfinish2a(S,B,A,CI) = CI \in cesfin2(nw(S)) .
  ceq nw(fkSfinish2a(S,B,A,CI)) = 
      (sf2M(intruder,B,A,CI) , nw(S)) 
    if c-fkSfinish2a(S,B,A,CI) .
  eq ur(fkSfinish2a(S,B,A,CI)) = ur(S) .
  eq uk(fkSfinish2a(S,B,A,CI)) = uk(S) .
  eq ui(fkSfinish2a(S,B,A,CI)) = ui(S) .
  eq ss(fkSfinish2a(S,B,A,CI),A2,B2,I) = ss(S,A2,B2,I) .
  ceq fkSfinish2a(S,B,A,CI) = S 
    if not c-fkSfinish2a(S,B,A,CI) .

-- fake client finish2 - case b
  op c-fkCfinish2b : Sys Pms -> Bool
  eq c-fkCfinish2b(S,PMS) = PMS \in cpms(nw(S)) .
  ceq nw(fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS)) =
      (cf2M(intruder,A,B,encFin(
          prf-ckey(prf-ms(PMS,R,R2), R,R2),
          prf-cfin2(prf-ms(PMS,R,R2),
            hFin2(A,B,R,I,CSs,R2,CS))
        )) , nw(S)) 
    if c-fkCfinish2b(S,PMS) .
  eq ur(fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS)) = ur(S) .
  eq uk(fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS)) = uk(S) .
  eq ui(fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS)) = ui(S) .
  eq ss(fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS),A2,B2,I) 
    = ss(S,A2,B2,I) .
  ceq fkCfinish2b(S,A,B,R,I,CSs,R2,PMS,CS) = S 
    if not c-fkCfinish2b(S,PMS) .

-- fake server finish2 - case b
  op c-fkSfinish2b : Sys Pms -> Bool
  eq c-fkSfinish2b(S,PMS) = PMS \in cpms(nw(S)) .
  ceq nw(fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS)) =
      (sf2M(intruder,B,A,encFin(
          prf-skey(prf-ms(PMS,R,R2), R,R2),
          prf-sfin2(prf-ms(PMS,R,R2),
            hFin2(A,B,R,I,CSs,R2,CS))
        )) , nw(S)) 
    if c-fkSfinish2b(S,PMS) .
  eq ur(fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS)) = ur(S) .
  eq uk(fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS)) = uk(S) .
  eq ui(fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS)) = ui(S) .
  eq ss(fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS),A2,B2,I)
    = ss(S,A2,B2,I) .
  ceq fkSfinish2b(S,B,A,R,I,CSs,R2,PMS,CS) = S 
    if not c-fkSfinish2b(S,PMS) .

-- abbreviation of server's certificate
  op scertAb : Prin -> Cert
  eq scertAb(B) = cert(B,pubKey(B),sign(ca,B,pubKey(B))) .

-- abbreviation of enc_PriS(H(R_C, R_S, PublicKeyShare_S))
  op shashkeyAb : Prin PriKeyEx Rand Rand -> Cipher
  eq shashkeyAb(B,K,R,R2) = encParams(priKey(B), R,R2,pubKeyEx(K,B)) .

  -- eq existPriSKexM(PMS,nw(S)) = false .
}

mod INV {
  pr(TLS12)

  op inv1 : Sys Pms -> Bool
  op inv2 : Sys Pms -> Bool
  op inv3 : Sys Pms -> Bool

  var S : Sys
  vars A A1 B B1 B2 B3 : Prin
  vars R R1 R2 : Rand
  var CS : CipherSuite
  var CSs : CipherSuites
  var I : Sid
  var PMS : Pms
  vars PK PK2 PK3 : PubKeyEx
  vars K K2 : PriKeyEx
  var CI : Cipher

  eq inv1(S,PMS) = (PMS \in cpms(nw(S))
    implies client(PMS) = intruder) .
  
  eq inv2(S,PMS) = (existPriCKexM(PMS,nw(S)) 
    implies client(PMS) = intruder) .

  eq inv3(S,PMS) = (existPriSKexM(PMS,nw(S)) = false) .

  op inv4 : Sys Prin Prin Prin Rand CipherSuites Rand CipherSuite Sid PubKeyEx Cipher PubKeyEx PubKeyEx PriKeyEx -> Bool
  eq inv4(S,A,B,B1,R,CSs,R2,CS,I,PK,CI,PK2,PK3,K) = 
    ((not (A = intruder) and
      sfM(B1,B,A,encFin(
        prf-skey(prf-ms(pms(PK,K,A,B),R,R2), R,R2),
        prf-sfin(prf-ms(pms(PK,K,A,B),R,R2),
          hFin(A,B,R,CSs, R2,CS,I, 
          cert(B,pubKey(B),sign(ca,B,pubKey(B))),
          PK2,CI,PK3)))) \in nw(S))
    implies 
      sfM(B,B,A,encFin(
        prf-skey(prf-ms(pms(PK,K,A,B),R,R2), R,R2),
        prf-sfin(prf-ms(pms(PK,K,A,B),R,R2),
          hFin(A,B,R,CSs, R2,CS,I, 
          cert(B,pubKey(B),sign(ca,B,pubKey(B))),
          PK2,CI,PK3)))) \in nw(S) ) .

  op inv5 : Sys Prin Prin Rand CipherSuites Rand CipherSuite Sid PubKeyEx Cipher PubKeyEx PubKeyEx PriKeyEx -> Bool
  eq inv5(S,A,B,R,CSs,R2,CS,I,PK,CI,PK2,PK3,K) = 
    ((not (A = intruder) and
      encFin(
        prf-skey(prf-ms(pms(PK,K,A,B),R,R2), R,R2),
        prf-sfin(prf-ms(pms(PK,K,A,B),R,R2),
          hFin(A,B,R,CSs, R2,CS,I, 
          cert(B,pubKey(B),sign(ca,B,pubKey(B))),
          PK2,CI,PK3))) \in cesfin(nw(S)))
    implies 
      sfM(B,B,A,encFin(
        prf-skey(prf-ms(pms(PK,K,A,B),R,R2), R,R2),
        prf-sfin(prf-ms(pms(PK,K,A,B),R,R2),
          hFin(A,B,R,CSs, R2,CS,I, 
          cert(B,pubKey(B),sign(ca,B,pubKey(B))),
          PK2,CI,PK3)))) \in nw(S) ) .
}