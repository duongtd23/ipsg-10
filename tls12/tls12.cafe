  eq (chM(A',A,B,R,CSs) = sf2M(A2',A2,B2,CI2)) = false .

  eq (shM(A',A,B,R,CS,I) = shM(A2',A2,B2,R2,CS2,I2))
    = (A' = A2' and A = A2 and B = B2 and R = R2 and 
      CS = CS2 and I = I2) .
  eq (shM(A',A,B,R,CS,I) = scertM(A2',A2,B2,CERT2)) = false .
  eq (shM(A',A,B,R,CS,I) = skexM(A2',A2,B2,PK2,CI2)) = false .
  eq (shM(A',A,B,R,CS,I) = ckexM(A2',A2,B2,PK2)) = false .
  eq (shM(A',A,B,R,CS,I) = cfM(A2',A2,B2,CI2)) = false .
  eq (shM(A',A,B,R,CS,I) = sfM(A2',A2,B2,CI2)) = false .
  eq (shM(A',A,B,R,CS,I) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (shM(A',A,B,R,CS,I) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (shM(A',A,B,R,CS,I) = cf2M(A2',A2,B2,CI2)) = false .
  eq (shM(A',A,B,R,CS,I) = sf2M(A2',A2,B2,CI2)) = false .

  eq (scertM(A',A,B,CERT) = scertM(A2',A2,B2,CERT2)) 
    = (A' = A2' and A = A2 and B = B2 and CERT = CERT2) .
  eq (scertM(A',A,B,CERT) = skexM(A2',A2,B2,PK2,CI2)) = false .
  eq (scertM(A',A,B,CERT) = ckexM(A2',A2,B2,PK2)) = false .
  eq (scertM(A',A,B,CERT) = cfM(A2',A2,B2,CI2)) = false .
  eq (scertM(A',A,B,CERT) = sfM(A2',A2,B2,CI2)) = false .
  eq (scertM(A',A,B,CERT) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (scertM(A',A,B,CERT) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (scertM(A',A,B,CERT) = cf2M(A2',A2,B2,CI2)) = false .
  eq (scertM(A',A,B,CERT) = sf2M(A2',A2,B2,CI2)) = false .

  eq (skexM(A',A,B,PK,CI) = skexM(A2',A2,B2,PK2,CI2)) 
    = (A' = A2' and A = A2 and B = B2 and PK = PK2 and 
      CI = CI2) .
  eq (skexM(A',A,B,PK,CI) = ckexM(A2',A2,B2,PK2)) = false .
  eq (skexM(A',A,B,PK,CI) = cfM(A2',A2,B2,CI2)) = false .
  eq (skexM(A',A,B,PK,CI) = sfM(A2',A2,B2,CI2)) = false .
  eq (skexM(A',A,B,PK,CI) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (skexM(A',A,B,PK,CI) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (skexM(A',A,B,PK,CI) = cf2M(A2',A2,B2,CI2)) = false .
  eq (skexM(A',A,B,PK,CI) = sf2M(A2',A2,B2,CI2)) = false .

  eq (ckexM(A',A,B,PK) = ckexM(A2',A2,B2,PK2)) 
    = (A' = A2' and A = A2 and B = B2 and PK = PK2) .
  eq (ckexM(A',A,B,PK) = cfM(A2',A2,B2,CI2)) = false .
  eq (ckexM(A',A,B,PK) = sfM(A2',A2,B2,CI2)) = false .
  eq (ckexM(A',A,B,PK) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (ckexM(A',A,B,PK) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (ckexM(A',A,B,PK) = cf2M(A2',A2,B2,CI2)) = false .
  eq (ckexM(A',A,B,PK) = sf2M(A2',A2,B2,CI2)) = false .

  eq (cfM(A',A,B,CI) = cfM(A2',A2,B2,CI2)) 
    = (A' = A2' and A = A2 and B = B2 and CI = CI2) .
  eq (cfM(A',A,B,CI) = sfM(A2',A2,B2,CI2)) = false .
  eq (cfM(A',A,B,CI) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (cfM(A',A,B,CI) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (cfM(A',A,B,CI) = cf2M(A2',A2,B2,CI2)) = false .
  eq (cfM(A',A,B,CI) = sf2M(A2',A2,B2,CI2)) = false .

  eq (sfM(A',A,B,CI) = sfM(A2',A2,B2,CI2)) 
    = (A' = A2' and A = A2 and B = B2 and CI = CI2) .
  eq (sfM(A',A,B,CI) = ch2M(A2',A2,B2,R2,I2,CSs2)) = false .
  eq (sfM(A',A,B,CI) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (sfM(A',A,B,CI) = cf2M(A2',A2,B2,CI2)) = false .
  eq (sfM(A',A,B,CI) = sf2M(A2',A2,B2,CI2)) = false .

  eq (ch2M(A',A,B,R,I,CSs) = ch2M(A2',A2,B2,R2,I2,CSs2))
    = (A' = A2' and A = A2 and B = B2 and R = R2 and 
      CSs = CSs2 and I = I2) .
  eq (ch2M(A',A,B,R,I,CSs) = sh2M(A2',A2,B2,R2,I2,CS2)) = false .
  eq (ch2M(A',A,B,R,I,CSs) = cf2M(A2',A2,B2,CI2)) = false .
  eq (ch2M(A',A,B,R,I,CSs) = sf2M(A2',A2,B2,CI2)) = false .

  eq (sh2M(A',A,B,R,I,CS) = sh2M(A2',A2,B2,R2,I2,CS2))
    = (A' = A2' and A = A2 and B = B2 and R = R2 and 
      CS = CS2 and I = I2) .
  eq (sh2M(A',A,B,R,I,CS) = cf2M(A2',A2,B2,CI2)) = false .
  eq (sh2M(A',A,B,R,I,CS) = sf2M(A2',A2,B2,CI2)) = false .

  eq (cf2M(A',A,B,CI) = cf2M(A2',A2,B2,CI2))
    = (A' = A2' and A = A2 and B = B2 and CI = CI2) .
  eq (cf2M(A',A,B,CI) = sf2M(A2',A2,B2,CI2)) = false .

  eq (sf2M(A',A,B,CI) = sf2M(A2',A2,B2,CI2))
    = (A' = A2' and A = A2 and B = B2 and CI = CI2) .


